{
  "name": "metrognome",
  "version": "1.2.4",
  "description": "Clean and simple metronome app",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile",
    "metronome",
    "music"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "sync": "npm run build && cap sync",
    "open": "cap open android",
    "version": "python scripts/bump_version.py",
    "publish": "npm run build && python scripts/publish.py"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "^7.0.0",
    "@capacitor/android": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "leancloud-storage": "^4.15.2"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.1",
    "vite": "^6.3.2"
  },
  "author": "Tony S.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lord-Turmoil/metrognome"
  },
  "bugs": {
    "url": "https://github.com/Lord-Turmoil/metrognome/issues"
  }
}
